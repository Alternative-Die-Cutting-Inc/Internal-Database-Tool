<header>
         <area>
               <name>Job Information</name>
               <textfield>Quote Number</textfield>
               <textfield>Author</textfield>
               <textfield>Attention</textfield>
               <textfield>Job Name</textfield>
               <textfield>Description</textfield>
         </area>
         
         <area>
               <name>Customer Information</name>
               <textarea default="1.00" cost="false">Rate</textarea>
               <dropdown>Customer</dropdown>
         </area>
         
         <area>
               <name>Main Notes</name>
               <textarea display="public">Notes</textarea>
         </area>
</header>







